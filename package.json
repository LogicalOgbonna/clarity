{
  "name": "clarity",
  "version": "1.0.1",
  "description": "AI-Powered Terms of Service Analyzer",
  "private": true,
  "repository": "https://github.com/LogicalOgbonna/clarity.git",
  "author": {
    "name": "Ogbonna Arinze",
    "email": "arinzeogbo@gmail.com",
