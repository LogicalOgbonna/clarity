  },
  "engines": {
    "node": ">=22.0.0",
    "yarn": ">= 1.0.0"
  },
  "scripts": {
    "dev:chrome": "concurrently \"cross-env NODE_ENV=development cross-env TARGET_BROWSER=chrome webpack --watch\" \"npm run core:dev\"",
    "dev:firefox": "concurrently \"cross-env NODE_ENV=development cross-env TARGET_BROWSER=firefox webpack --watch\" \"npm run core:dev\"",
    "dev:opera": "concurrently \"cross-env NODE_ENV=development cross-env TARGET_BROWSER=opera webpack --watch\" \"npm run core:dev\"",
    "core:dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register --project core/tsconfig.json --watch core core/index.ts",
    "build:chrome": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=chrome webpack",
    "build:firefox": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=firefox webpack",
    "build:opera": "cross-env NODE_ENV=production cross-env TARGET_BROWSER=opera webpack",
    "build": "yarn run build:chrome && yarn run build:firefox && yarn run build:opera",
    "build:core": "tsc --project core/tsconfig.json && tsc-alias --project core/tsconfig.json",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "web:dev": "cd web && next dev",
    "web:build": "cd web && next build",
    "web:start": "cd web && next start",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push",
    "prisma:pull": "prisma db pull",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:reset": "prisma migrate reset",
    "prisma:seed": "prisma db seed"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.42",
